{
  "defaultSeverity": "error",
  "extends": "tslint-config-airbnb",

  "jsRules": {},
  "rules": {
    "brace-style": "stroustrop",
    "curly": [false],
    "function-name": [false],
    "import-name": false,
    "interface-name": false,
    "max-line-length": [true, 140],
    "no-else-after-return": false,
    "object-shorthand-properties-first": false,
    "ordered-imports": false,
    "prefer-template": false,
    "quotemark": false,
    "trailing-comma": [true, {
      "multiline": {
        "objects": "always",
        "arrays": "always",
        "functions": "never",
        "typeLiterals": "ignore"
      },
      "single-line": "never"
    }],
    "variable-name": [true,
      "allow-snake-case"
    ],
    "whitespace": [true,
        "check-decl",
        "check-operator",
        "check-module",
        "check-separator",
        "check-rest-spread",
        "check-type",
        "check-typecast",
        "check-type-operator",
        "check-preblock"
    ]
  },
  "rulesDirectory": []
}
